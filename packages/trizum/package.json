{
  "name": "trizum",
  "description": "",
  "keywords": [],
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "author": "Horus Lugo <hola@horus.dev> (https://horus.dev)",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "cleanup": "rm -rf dist",
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@horus.dev/eslint-config": "2.0.1",
    "@horus.dev/prettier-config": "1.0.0",
    "@horus.dev/tsconfig": "2.1.0",
    "eslint": "8.57.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "prettier": "@horus.dev/prettier-config",
  "dependencies": {
    "@automerge/automerge-repo": "1.2.1",
    "@opentf/obj-diff": "jsr:0.11.0",
    "@opentf/std": "jsr:0.12.0",
    "@takker/md5": "jsr:0.1.0",
    "dinero.js": "1.9.1",
    "ulidx": "2.4.1"
  }
}
