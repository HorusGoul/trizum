{
  "name": "trizum",
  "compatibility_date": "2025-11-18",
  "main": "api/worker.ts",
  "assets": {
    "directory": "./dist/client",
    "not_found_handling": "single-page-application",
    "binding": "ASSETS",
    "run_worker_first": ["/api/*"]
  },
  "routes": [
    {
      "pattern": "trizum.app",
      "custom_domain": true
    }
  ]
}
